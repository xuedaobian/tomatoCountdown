{
  "name": "tomatocountdown",
  "displayName": "tomatoCountdown",
  "description": "focus and relax",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
      "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
      "commands": [
          {
              "command": "pomodoro.start",
              "title": "Start Pomodoro Timer"
          },
          {
              "command": "pomodoro.stop",
              "title": "Stop Pomodoro Timer"
          },
          {
              "command": "pomodoro.pause",
              "title": "Pause Pomodoro"
          },
          {
              "command": "pomodoro.resume",
              "title": "Resume Pomodoro"
          },
          {
              "command": "pomodoro.showMenu",
              "title": "Show Pomodoro Menu"
          },
          {
              "command": "pomodoro.showRecords",
              "title": "Show Pomodoro Records"
          }
      ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
